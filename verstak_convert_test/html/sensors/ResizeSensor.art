  тип ResizedElement = интерфейс
  {
    защищено
    borderBoxSize: ReadonlyArray<ResizeObserverSize>

    защищено
    contentBoxSize: ReadonlyArray<ResizeObserverSize>

    защищено
    contentRect: DOMRectReadOnly

    защищено
    resizeData: Объект

  }


  тип ResizeSensor = объект на основе Sensor
  {
    /*(!) private */ защищено
    observer: ResizeObserver

    resizedElements: Array<ResizedElement>

    при создании()

    операция reset(): Ничего

    операция observeResizing(block: Block<Объект, Объект, Объект>, value: ДаНет, boxSizing: ResizeObserverBoxOptions = "content-box"): Ничего

    /*(!) protected */ 
    операция onResize(entries: Array<ResizeObserverEntry>): Ничего

    /*(!) protected */ 
    операция resize(entries: Array<ResizeObserverEntry>): Ничего

    /*(!) protected */ 
    операция doReset(): Ничего
  }

}
